{% set label = [] %}
{% set value1 = [] %}
{% set value2 = [] %}
{% for eloValue in eloHistory1 %}
    {% set label = label|merge([eloValue.dateAsMonth]) %}
    {% set value1 = value1|merge([eloValue.eloRatingAvg | round]) %}
{% endfor %}

{% for eloValue in eloHistory2 %}
    {% set label = label|merge([eloValue.dateAsMonth]) %}
    {% set value2 = value2|merge([eloValue.eloRatingAvg | round]) %}
{% endfor %}

<div style="width:100%;" >
    <canvas
            class="elo-history-canvas"
            id="elo-history-canvas"
            data-label="{{ label|join(',') }},"
            data-value1="{{ value1|join(',') }},"
            data-value2="{{ value2|join(',') }},">
    </canvas>
</div>
