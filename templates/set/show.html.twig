    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ set.id }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ set.createdAt ? set.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ set.updatedAt ? set.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
